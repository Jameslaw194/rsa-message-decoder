    .scope S_0x15390b130;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x153925270_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x153925270_0;
    %inv;
    %store/vec4 v0x153925270_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %end;
