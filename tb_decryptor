    %subi 1, 0, 32;
    %wait E_0x15390b9b0;
    %jmp T_2.3;
T_2.4 ;
    %pop/vec4 1;
    %vpi_call 2 55 "$write", "%c", v0x153925300_0 {0 0 0};
T_2.2 ; for-loop step statement
    %load/vec4 v0x1539254f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x1539254f0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ; for-loop exit label
    %delay 10000, 0;
    %vpi_call 2 60 "$write", "\012" {0 0 0};
    %vpi_call 2 61 "$finish" {0 0 0};
    %end;
    .thread T_2;
