    %store/vec4 v0x1539254f0_0, 0, 32;
T_2.0 ; Top of for-loop
    %load/vec4 v0x1539254f0_0;
    %cmpi/s 20, 0, 32;
	  %jmp/0xz T_2.1, 5;
    %ix/getv/s 4, v0x1539254f0_0;
    %load/vec4a v0x153925460, 4;
    %store/vec4 v0x153925390_0, 0, 16;
    %pushi/vec4 15, 0, 32;
T_2.3 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_2.4, 5;
