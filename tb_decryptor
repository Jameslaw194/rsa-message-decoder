    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x153924e50_0;
    %load/vec4 v0x153924fb0_0;
    %cmp/ne;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x153924e50_0;
    %assign/vec4 v0x153924fb0_0, 0;
