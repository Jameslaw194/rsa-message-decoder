    %pad/u 32;
    %assign/vec4 v0x153905530_0, 0;
    %pushi/vec4 1, 0, 32;
    %assign/vec4 v0x153925170_0, 0;
    %pushi/vec4 2753, 0, 16;
    %assign/vec4 v0x153924f00_0, 0;
    %pushi/vec4 12, 0, 5;
    %assign/vec4 v0x153924b60_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x153924cc0_0, 0;
T_0.4 ;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x153924b60_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_0.6, 5;
    %load/vec4 v0x153924f00_0;
    %parti/s 1, 0, 2;
